{
	"name": "Artemis",
	"description": "SSL/TLS Security Level Display",
	"version": "0.2",
	"manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery.min.js"]
        }
    ],
    
    "background": {
        "scripts": ["background.js"]
    },
    
	"browser_action": {
		"default_icon": "default.png",
		"default_popup": "detailed.html"
	},

	"permissions": [
		"storage",
		"tabs"
	]
}
